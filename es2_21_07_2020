}}

}







float media(Item *list){

int n=10;
int somma=0;

Item *h=list;
while(list){
somma=somma+list->voto;
list=list-<next;

}
list=h;

return (float) (somma/10);

}




void display(Item *list, float media) {
Item *h=list;

while (list){

if(list->voto < media) {
printf("%d ", list->voto);
}
list=list->next;

}

list=h;
}
